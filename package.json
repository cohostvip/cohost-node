{
  "name": "@cohostvip/cohost-node",
  "version": "1.0.0-beta.2",
  "description": "Cohost API wrapper",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/cohostvip/cohost-node",
  "author": "Cohost <support@cohost.vip> (https://cohost.vip)",
  "repository": {
    "type": "git",
    "url": "https://github.com/cohostvip/cohost-node.git"
  },
  "bugs": {
    "url": "https://github.com/cohostvip/cohost-node/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
