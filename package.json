  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitest/ui": "^3.1.1",
    "tsup": "^8.4.0",
    "typescript": "5.8.2",
    "vite": "6.2.2",
    "vitest": "^3.1.1"
  }
